generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("MONGO_URL")
}

model News {
  id          String    @id @default(auto()) @map("_id") @db.ObjectId
  title       String?
  link        String?   @unique
  rssUrl      String
  pubDate     DateTime?
  description String?
  thumbnail   String?
  createdAt   DateTime  @default(now())
}
